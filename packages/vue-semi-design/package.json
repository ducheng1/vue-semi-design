{
  "name": "vue-semi-design",
  "type": "module",
  "version": "0.0.1-alpha.1",
  "packageManager": "pnpm@10.15.0",
  "description": "Vue3 component library in semi design style",
  "author": {
    "name": "ducheng1",
    "email": "w729567588@163.com"
  },
  "license": "MIT",
  "homepage": "https://semi.dcwedu.top",
  "repository": {
    "type": "git",
    "url": "https://github.com/ducheng1/vue-semi-design.git"
  },
  "bugs": {
    "url": "https://github.com/ducheng1/vue-semi-design/issues"
  },
  "keywords": [
    "semi design",
