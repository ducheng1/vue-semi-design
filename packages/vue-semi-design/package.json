{
  "name": "vue-semi-design",
  "type": "module",
  "version": "0.0.1-alpha.3",
  "description": "Vue3 component library in semi design style",
  "author": {
    "name": "ducheng1",
    "email": "w729567588@163.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/ducheng1/vue-semi-design",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ducheng1/vue-semi-design.git"
  },
  "bugs": {
    "url": "https://github.com/ducheng1/vue-semi-design/issues"
  },
  "keywords": [
    "semi design",
    "vue",
    "component library",
    "ui library"
  ],
  "exports": {
    ".": {
      "types": "./dist/es/index.d.ts",
      "import": "./dist/es/index.js"
    },
    "./styles/*": "./dist/es/styles/*",
    "./umd/*": "./dist/umd/*",
    "./es/*": "./dist/es/*"
  },
  "main": "./dist/umd/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/es/index.d.ts",
  "style": "./dist/umd/index.css",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "pnpm run clean && rslib build --watch",
    "build": "pnpm run clean && rslib build && vue-tsc",
    "release": "bumpp --preid alpha --tag 'v' --push",
    "update-version": "tsx ./scripts/update-version.ts && git add ./src/version.ts && git commit -m 'chore: update version'",
    "postversion": "pnpm run update-version",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.5.3",
    "@rsbuild/plugin-sass": "^1.4.0",
    "@rslib/core": "^0.12.4",
    "rsbuild-plugin-unplugin-vue": "^0.1.0",
    "sass": "^1.92.0",
    "typescript": "^5.9.2",
    "vue": "^3.5.21",
    "vue-tsc": "^3.0.6"
  }
}
