    "pnpm": ">=10"
  },
  "scripts": {
    "dev": "rslib build --watch",
    "build": "rslib build && vue-tsc",
    "publish": "npm publish",
    "prepublish": "pnpm run build"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@rsbuild/core": "^1.5.3",
    "@rsbuild/plugin-sass": "^1.4.0",
    "@rslib/core": "^0.12.4",
    "rsbuild-plugin-unplugin-vue": "^0.1.0",
    "sass": "^1.92.0",
